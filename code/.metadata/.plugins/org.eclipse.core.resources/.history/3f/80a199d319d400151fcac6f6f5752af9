package models.entidades;

import org.jongo.marshall.jackson.oid.MongoObjectId;

public class Maquina {
	@MongoObjectId
	private String id;
	private String nombre;
	private String ip;
	private java.util.List<Aplicacion> aplicaciones;
	
	public String getNombre() {
		return nombre;
	}
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	private String getIp() {
		return ip;
	}
	public void setIp(String ip) {
		this.ip = ip;
	}
	public java.util.List<Aplicacion> getAplicaciones() {
		return aplicaciones;
	}
	public void setAplicaciones(java.util.List<Aplicacion> aplicaciones) {
		this.aplicaciones = aplicaciones;
	}
	

}
