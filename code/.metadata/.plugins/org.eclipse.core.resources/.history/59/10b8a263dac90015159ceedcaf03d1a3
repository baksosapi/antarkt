# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET		/api/usuarios/id/:id		controllers.APIUsuario.findById(id:String)
GET		/api/usuarios/nombre/:name	controllers.APIUsuario.findByName(name:String)
GET		/api/usuarios/insert/:name	controllers.APIUsuario.insert(name:String)
#POST	/api/usuarios				controllers.APIAgente.crear()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
